<local:DDCVSwapableScreen x:Class="DodocoTales.SR.Gui.Views.Screens.DDCVHomeScreen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DodocoTales.SR.Gui.Views.Screens" 
             xmlns:cards="clr-namespace:DodocoTales.SR.Gui.Views.Cards"
             mc:Ignorable="d" 
             d:DesignHeight="800" d:DesignWidth="800">
    <Grid Margin="5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <cards:DDCVHomeSceneCharEventCard  x:Name="CharEvent"/>
       
        <cards:DDCVHomeScenePermanentCard  x:Name="Permanant" Grid.Column="1"/>
        <cards:DDCVHomeSceneLCEventCard x:Name="LCEvent" Grid.Column="2"/>
    </Grid>
</local:DDCVSwapableScreen>
